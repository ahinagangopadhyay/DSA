class Solution {
public:
    vector<int> getFinalState(vector<int>& nums, int k, int multiplier) {
        for(int i=1;i<=k;i++)
        {
          int  mini=nums[0];
          int  minindex=0;
            for(int j=0;j<nums.size();j++)
            {
                if(nums[j]<mini)
                {
                    mini=nums[j];;
                    minindex=j;
                }

            }
            nums[minindex]*=multiplier;
        }
        return nums;
        
    }
};
